{
  "name": "bolix_notifications",
  "version": "1.0.0",
  "description": "incoming server webhook notifications",
  "main": "./production/index.js",
  "author": "Alexandre Henrique",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc",
    "start": "NODE_ENV=prod node -r dotenv/config ./production/index.js",
    "dev": "NODE_ENV=dev ts-node-dev -r dotenv/config src/index.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "generate": "npx prisma generate",
    "pull": "npx prisma db pull",
    "push": "yarn prisma db push",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:dev": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@prisma/client": "5.13.0",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "gn-api-sdk-typescript": "^2.0.1",
    "mysql2": "^3.6.0",
    "prisma": "^5.2.0",
    "sdk-typescript-apis-efi": "^1.0.2",
    "sequelize": "^6.32.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "eslint": "^9.1.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^3.0.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
